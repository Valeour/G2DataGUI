<UserControl x:Class="G2DataGUI.UI.Views.SectionMenuBar"
			 xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:G2DataGUI.UI.ViewModels"
             mc:Ignorable="d"
			 Focusable="True"
			 x:CompileBindings="True"
			 x:DataType="vm:MainWindowViewModel">
	<DockPanel HorizontalAlignment="Stretch">
		<Menu Name="SectionMenu"
			Height="50"
			Margin="0"
			HorizontalAlignment="Left">
			<Menu.ItemsPanel>
				<ItemsPanelTemplate>
					<DockPanel Margin="0"
								HorizontalAlignment="Stretch" />
				</ItemsPanelTemplate>
			</Menu.ItemsPanel>
			<MenuItem Header="Moves" />
			<MenuItem Header="Skills" />
			<MenuItem Header="Eggs" />
			<MenuItem Header="Books" />
			<MenuItem Header="Specials" />
			<MenuItem Header="Items" />
			<MenuItem Header="Enemies" />
			<MenuItem Header="Bosses" />
			<MenuItem Header="Maps" />
		</Menu>
	</DockPanel>
</UserControl>